{
    "name": "kif/doctrine_typescript_bundle",
    "type": "symfony-bundle",
    "description": "convert doctrine entities into typescript classes",
    "keywords": [
        "symfony2",
        "cli",
        "typescript",
        "doctrine"
    ],
    "homepage": "https://github.com/Kifah/KifDoctrineToTypescriptBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kifah Abbad",
            "email": "kifah.abbad@devbliss.com",
            "homepage": "http://www.devbliss.com",
            "role": "Senior Backend Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "~2.1",
        "jms/serializer-bundle": "0.13.*@dev"

    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "Kif\\DoctrineToTypescriptBundle\\": ""
        }
    },
    "target-dir": "Kif/DoctrineToTypescriptBundle"
}